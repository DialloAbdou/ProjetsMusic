@model ListMusicViewModel;

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name of Music</th>
            <th>Artist</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var music in Model.ListMusic)
        {
            <tr>
                <td>@music.Name</td>
                <td>@music.Artist.Name</td>
            </tr>

        }
    </tbody>
</table>